{
  "name": "webpack-5",
  "version": "1.0.0",
  "description": "curso de webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi/webpack.config.js",
    "build:css-loaders": "webpack --config ./css-loaders/webpack.config.js",
    "build:plugins": "webpack --config ./webpack-plugins/webpack.config.js",
    "build:dev": "webpack serve --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack serve --config ./babel/webpack.config.js",
    "build:react": "webpack serve --config ./react/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Andres Bonett <andresbonettm@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.2.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  }
